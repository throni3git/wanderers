{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": ["IE >= 11"]
				},
				"useBuiltIns": "entry"
			}
		],
		"@babel/preset-typescript",
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread",
		"@babel/plugin-transform-runtime",
		"@babel/plugin-transform-arrow-functions"
	]
}
